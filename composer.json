{
    "name": "systemfailure/crypto-currency-exchanges-trader",
    "type": "library",
    "description": "CryptoCurrencyExchangesTrader",
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "6.3.*",
        "symfony/var-dumper": "v4.2.2",
        "sentry/sentry": "1.9.*",
        "symfony/dependency-injection": "^4.2",
        "symfony/options-resolver": "^4.2",
        "symfony/validator": "^4.2",
        "vlucas/phpdotenv": "^3.3",
        "symfony/console": "^4.2",
        "symfony/config": "^4.2",
        "symfony/cache": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.11.1",
        "phpunit/phpunit": "^7.5",
        "vimeo/psalm": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "systemfailure",
            "email": "kefzce@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test" : "vendor/bin/phpunit --colors=always",
        "fix": "vendor/bin/php-cs-fixer fix"
    }
}
